<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Park ve Bahçeler Şube Müdürlüğü</title>
    <link rel="stylesheet" href="~/Content/adminThema/css/style.default.css" type="text/css" />
    <link rel="stylesheet" href="~/Content/adminThema/prettify/prettify.css" type="text/css" />

    <meta charset="utf-8" />
    <link href="~/favicon.ico" rel="shortcut icon" type="image/x-icon" />
    <meta name="viewport" content="width=device-width" />
    @Styles.Render("~/Content/css")

</head>

<body>

    <div id="mainwrapper" class="mainwrapper">
        <div class="header">@Html.Partial("_LoginPartialAdmin") </div>
        <div class="leftpanel">@Html.Partial("_LeftMenuPartialAdmin")</div>
        <div class="righpanel">
            <div class="rightpanel" style="margin-left: 260px;">
                <ul class="breadcrumbs">
                    <li><a href="/Admin/Index"><i class="iconfa-home"></i></a><span class="separator"></span></li>
                    <li><a href="/Admin/Index">Admin</a> <span class="separator"></span></li>
                </ul>
                <div class="maincontent">
                    <div class="maincontentinner">

                        @RenderBody()

                        @Html.Partial("_FooterAdmin")
                    </div>
                    <!--maincontentinner-->
                </div>
                <!--maincontent-->
            </div>
        </div>
        <!--mainwrapper-->

    </div>


    @Scripts.Render("~/bundles/modernizr")
    <script src="~/Scripts/jquery-2.1.4.min.js"></script>
    <script src="~/Scripts/angular.min.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>
    <script src="~/Scripts/amplify.min.js"></script>
    
    <script type="text/javascript" src="~/Content/adminThema/prettify/prettify.js"></script>
    <script type="text/javascript" src="~/Content/adminThema/js/modernizr.min.js"></script>
    <script type="text/javascript" src="~/Content/adminThema/js/bootstrap.min.js"></script>
    <script type="text/javascript" src="~/Content/adminThema/js/jquery.jgrowl.js"></script>
    <script type="text/javascript" src="~/Content/adminThema/js/jquery.alerts.js"></script>

    <script type="text/javascript" src="~/Content/adminThema/js/jquery.slimscroll.js"></script>
    <script type="text/javascript" src="~/Content/adminThema/js/custom.js"></script>
    <script type="text/javascript" src="~/Content/adminThema/js/elements.js"></script>




    @Scripts.Render("~/bundles/dataservice")
    @*   @Scripts.Render("~/bundles/adminTheme")*@

    @*   @Scripts.Render("~/bundles/applicationAngular")*@
    @Scripts.Render("~/bundles/application")

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/jqueryui")
    @RenderSection("scripts", required: false)



    <script src="~/Scripts/jquery.mousewheel-3.0.6.pack.js"></script>
    <script src="~/Scripts/jquery.fancybox.js?v=2.1.3"></script>
    <link href="~/Content/jquery.fancybox.css?v=2.1.2" rel="stylesheet" media="screen" />
    <link href="~/Content/jquery.fancybox.css" rel="stylesheet" />
    <script type="text/javascript">
        serviceInitializer.init();
    </script>
</body>
</html>
