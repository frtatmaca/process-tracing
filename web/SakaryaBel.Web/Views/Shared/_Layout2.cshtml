<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Park ve Bahçeler Şube Müdürlüğü</title>
    <link rel="stylesheet" href="~/Content/adminThema/css/style.default.css" type="text/css" />
    <link rel="stylesheet" href="~/Content/adminThema/prettify/prettify.css" type="text/css" />

    <meta charset="utf-8" />
    <link href="~/favicon.ico" rel="shortcut icon" type="image/x-icon" />
    <meta name="viewport" content="width=device-width" />
    @Styles.Render("~/Content/css")
    @Styles.Render("~/Content/adminTheme")
    @Scripts.Render("~/bundles/modernizr")
    @Styles.Render("~/Content/themes/base/css")

