
<style type="text/css">

ul.menu {
    list-style-type: none;
    padding: 0;
    margin: 0;
}

ul.menu li {
	display: inline;
}

ul.menu li {
  text-decoration: none;  
  /*color: #1B1B1B;*/
  background-color: #EEE;  
  font-size: 11px; 
} 

ul.menu li a {
        /* float: left;*/
    padding: 4px 12px;
    line-height: 20px;
    text-decoration: none;
    background-color: #fff;
    border: 1px solid #ddd;
    border-left-width: 0;
}

.menu a:hover {
    background-color: #005f5f;
}
 
.menu li.active a {
  background-color: #b6ff00;
  color: #444;
  cursor: default;
}
</style>

<div class="row-fluid">
    <div class="span6">
        <div class="dataTables_info">
            @Html.Raw(String.Format("Showing {0} to {1} of {2} entries", "<span data-bind='text: firstIndex'></span>", "<span data-bind='text: lastIndex'></span>", "<span data-bind='text: totalCount'></span>"))
        </div>
    </div>
    <div class="dataTables_info">
        <div class="dataTables_paginate paging_bootstrap pagination">
            <ul id="ul_paging" class="menu">               
            </ul>
        </div>
    </div>
</div>
